{
  "name": "camera-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build": "npm run lint && expo build",
    "lint": "eslint . --ext .js,.jsx",
    "prepare": "husky",
    "check": "npm run lint && npm run type-check && npm test",
    "type-check": "tsc --noEmit",
    "docs:uk": "set JSDOCTEMPLATE=uk&& jsdoc -c jsdoc.config.uk.json",
    "docs:en": "jsdoc -c jsdoc.config.en.json",
    "docs": "npm run docs:en && npm run docs:uk",
    "deploy-docs": "gh-pages -d docs"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/stack": "^7.0.12",
    "@sentry/react-native": "~6.10.0",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-react-native": "^1.0.0",
    "camera-app": "file:",
    "expo": "^53.0.4",
    "expo-camera": "~16.1.6",
    "expo-constants": "~17.1.4",
    "expo-dev-client": "~5.1.7",
    "expo-file-system": "~18.1.8",
    "expo-font": "~13.3.0",
    "expo-gl": "~15.1.5",
    "expo-gl-cpp": "^11.4.0",
    "expo-image-manipulator": "~13.1.5",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.4",
    "expo-localization": "~16.1.5",
    "expo-mail-composer": "~14.1.4",
    "expo-media-library": "~17.1.6",
    "expo-permissions": "^14.4.0",
    "expo-router": "~5.0.3",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.6",
    "expo-updates": "~0.28.11",
    "expo-web-browser": "~14.1.6",
    "i18n-js": "^4.5.1",
    "loglevel": "^1.9.2",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.1",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-logs": "^5.3.0",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.6",
