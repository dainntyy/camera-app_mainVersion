{
  "name": "camera-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build": "npm run lint && expo build",
    "lint": "eslint . --ext .js,.jsx",
    "prepare": "husky",
    "check": "npm run lint && npm run type-check && npm test",
    "type-check": "tsc --noEmit",
    "docs:uk": "set JSDOCTEMPLATE=uk&& jsdoc -c jsdoc.config.uk.json",
    "docs:en": "jsdoc -c jsdoc.config.en.json",
    "docs": "npm run docs:en && npm run docs:uk",
    "deploy-docs": "gh-pages -d docs"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/native": "^7.0.7",
    "@react-navigation/stack": "^7.0.12",
    "camera-app": "file:",
    "expo": "^52.0.11",
    "expo-camera": "~16.0.7",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.4",
    "expo-file-system": "~18.0.4",
    "expo-font": "~13.0.1",
    "expo-image-manipulator": "~13.0.5",
    "expo-image-picker": "~16.0.3",
    "expo-linking": "~7.0.3",
    "expo-media-library": "~17.0.3",
    "expo-permissions": "^14.4.0",
    "expo-router": "^4.0.9",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.4",
    "expo-web-browser": "~14.0.1",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.6",
    "@types/hammerjs": "^2.0.45",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/json-schema": "^7.0.15",
    "@types/prop-types": "^15.7.13",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/stack-utils": "^2.0.3",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "detox": "^20.28.0",
    "docdash": "^2.0.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-universe": "^14.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "expo-module-scripts": "^3.5.2",
    "flow-bin": "^0.266.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.2",
    "jsdoc": "^4.0.4",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "private": true,
  "detox": {
    "testRunner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "type": "ios.simulator",
        "binaryPath": "bin/ios/Debug-iphonesimulator/MyApp.app",
        "build": "xcodebuild ...",
        "device": {
          "type": "iPhone 11"
        }
      },
      "android.emu.debug": {
        "type": "android.emulator",
        "binaryPath": "bin/android/Debug/app-debug.apk",
        "build": "gradlew assembleDebug",
        "device": {
          "avdName": "Pixel_3"
        }
      }
    }
  }
}
