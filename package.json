    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "expo-module-scripts": "^3.5.2",
    "flow-bin": "^0.266.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.2",
    "jsdoc": "^4.0.4",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "private": true,
  "detox": {
    "testRunner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "type": "ios.simulator",
        "binaryPath": "bin/ios/Debug-iphonesimulator/MyApp.app",
        "build": "xcodebuild ...",
        "device": {
          "type": "iPhone 11"
        }
      },
      "android.emu.debug": {
        "type": "android.emulator",
        "binaryPath": "bin/android/Debug/app-debug.apk",
        "build": "gradlew assembleDebug",
        "device": {
          "avdName": "Pixel_3"
        }
      }
    }
  }
}
